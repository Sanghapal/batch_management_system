= simple_form_for addmition_path do |f|
  = f.hidden_field :student_id
  = f.input :admission_date, :as => :datepicker, :readonly => true
  = f.input :grade_id, :collection => Grade.all.entries, :value_method => :id, :label_method =>  :title, :input_html => {:id => "comgrade"}
  = f.input :sponsor_flag, as: :boolean
  = f.submit"submit"